{
	"name": "demo-angular",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/demo-angular/src",
	"projectType": "application",
	"prefix": "demo",
	"targets": {
		"build": {
			"executor": "@nativescript/nx:build",
			"options": {
				"noHmr": true,
				"production": true,
				"uglify": true,
				"release": true,
				"forDevice": true,
				"tsConfig": "apps/demo-angular/tsconfig.json"
			},
			"dependsOn": [
				{
					"target": "build.all",
					"projects": "dependencies"
				},
				{
					"target": "build.all",
					"projects": "dependencies"
				}
			]
		},
		"test": {
			"executor": "@nativescript/nx:test",
			"configurations": {
				"android": {},
				"ios": {}
			},
			"dependsOn": [
				{
					"target": "build.all",
					"projects": "dependencies"
				},
				{
					"target": "build.all",
					"projects": "dependencies"
				}
			]
		},
		"clean": {
			"executor": "@nativescript/nx:clean",
			"options": {}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"options": {
				"lintFilePatterns": ["apps/demo-angular/**/*.ts"]
			}
		},
		"debug": {
			"dependsOn": [
				{
					"target": "build.all",
					"projects": "nativescript-supabase"
				}
			],
			"executor": "@nativescript/nx:debug",
			"options": {
				"noHmr": true,
				"uglify": false,
				"release": false,
				"forDevice": false,
				"prepare": false
			}
		}
	},
	"tags": []
}
